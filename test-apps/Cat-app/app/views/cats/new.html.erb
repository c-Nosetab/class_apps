
<div class="content">
  <h1>Add Cat! --- MEEEOW!</h1>

  <%= form_tag '/cats', method: :post do %>
    <div>
      <%= label_tag :name %>
      <%= text_field_tag :name %>
    </div>
    <div>
      <%= label_tag :age %>
      <%= text_field_tag :age %>
    </div>
    <div>
      <%= label_tag :price %>
      <%= text_field_tag :price %>
    </div>
    <div>
      <%= label_tag :description %>
      <%= text_field_tag :description %>
    </div>
    <div>
      <%= label_tag :url %>
      <%= text_field_tag :url %>
    </div>
    <%= submit_tag :MEEEOW %>


  <% end %>

</div>

